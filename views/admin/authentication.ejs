  <title>Administrator</title>
</head>
<body>
  <div class="col-sm-12">

    <form>
      <div class="col-sm-12">
        <p class="h4 text-justify">2-Way Authentication</p>
      </div>
      <div class="col-sm-5">
        <input type="text" class="form-control btn-block">
      </div>

      <div class="col-sm-2">
        <input type="submit" class="btn btn-block btn-success" value="Verify">
      </div>

      <div class="col-sm-2">
        <a id="cancelRequest" class="btn btn-block btn-default" href="#">Cancel Request</a>
      </div>
    </form>
  </div>
</body>
