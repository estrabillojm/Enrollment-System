  <title>Administrator</title>
</head>
<body>


    <div class="col-sm-8 col-sm-offset-2 modFormBg pre-scrollable">

      <%if(student[0].status === 'forSave'){%>
      <div class="col-sm-12 modHeaderEnroll">
        <b><center>REQUIREMENTS</center></b>
      </div>
      <form id="formRequirements">
          <div class="row">
            <div class="col-sm-10 col-sm-offset-1">
              <div class="checkbox">

                <p><b>Mark all the requirements that are submitted by the student below.</b></p>

                <div class="checkbox">
                  <label><input class="incompleteReq" type="checkbox" value="FORM 138">Form 138</label>
                </div>

                <div class="checkbox">
                  <label><input class="incompleteReq" type="checkbox" value="FORM 137">Form 137 <span class="modTransText"><i>(For Transferees)</i></span></label>
                </div>

                <div class="checkbox">
                  <label><input class="incompleteReq" type="checkbox" value="PSA/NSO Birth Certificate">PSA/NSO Birth Certificate</label>
                </div>
                <div class="checkbox myCb">
                  <label><input class="incompleteReq" type="checkbox" value="Good Moral Certificate">Good Moral Certificate</label>
                </div>
                <div class="checkbox">
                  <label><input class="incompleteReq" type="checkbox" value="2x2 Picture">2x2 Picture</label>
                </div>
                <div class="checkbox">
                  <label><input class="incompleteReq" type="checkbox" value="NCAE Rating">NCAE Rating</label>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-sm-4 col-sm-offset-8">
              <a id="submitRequirements" class="btn btn-block btn-default buttonRequirements" href="#">Submit</a>
            </div>
          </div>
        </form>

        <%}%>







      <div class="col-sm-12 modHeaderEnroll">
        <b><center>PERSONAL INFORMATION</center></b>
      </div>



    <div class="row">
      <div class="col-sm-10 col-sm-offset-1">
        <b>LRN:</b>
        <input type="text" class="form-control btn-block modTextbox" id="lrn" value="<%-student[0].lrn%>" <%if(student[0].status==='forUpdate'){%><%-'disabled'%><%}%>>
      </div>
    </div>
    <br class="hidden-xs"/>

    <div class="row">
      <div class="col-sm-10 col-sm-offset-1">
        <b>Lastname:</b>
        <input type="text" class="form-control btn-block modTextbox" id="hiddenLastname" value="<%-student[0].lastname%>">
      </div>
    </div>

    <br class="hidden-xs"/>
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1">
        <b>Firstname:</b>
        <input type="text" class="form-control btn-block modTextbox" id="hiddenFirstname"  value="<%-student[0].firstname%>">
      </div>
    </div>

    <br class="hidden-xs"/>
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1">
        <b>Middlename:</b>
        <input type="text" class="form-control btn-block modTextbox" id="hiddenMiddlename" value="<%-student[0].middlename%>">
      </div>
    </div>
    <br class="hidden-xs"/>



    <div class="row">
      <div class="col-sm-5 col-sm-offset-1">
        <b>Year & Section:</b>
        <select class="form-control btn-block modTextbox" id="section">
          <%section.forEach(function(section){%>
            <option <%if(student[0].section===section.section && student[0].strand===section.strand){%><%-'selected '%><%}%> data-strand="<%-section.strand%>" value="<%-section.section%>"><%-section.section%> (<%-section.strand%>)</option>
          <%});%>
        </select>
      </div>



      <div class="col-sm-5">
        <b>Strands:</b>
        <select class="form-control btn-block modTextbox" id="strand">
          <%strand.forEach(function(strand){%>
            <option <%if(strand.strands === student[0].strand){%><%-'selected '%><%}%> value="<%-strand.strands%>"><%-strand.strands%></option>
          <%})%>
        </select>
      </div>
    </div>
    <br class="hidden-xs"/>




    <div class="row">
      <div class="col-sm-5 col-sm-offset-1">
        <b>Date of Birth:</b>
        <input type="date"  class="form-control btn-block modTextbox" id="dob" value="<%-student[0].dob%>" >
      </div>

      <div class="col-sm-5">
        <b>Phone Number:</b>
        <input type="tel" id="phone" pattern="[0]{1}[9]{1}[0-9]{2}[0-9]{3}[0-9]{4}"  value="<%-student[0].phone%>" class="form-control btn-block modTextbox" placeholder="Contact No.. (i.e 09123456789)">
      </div>
    </div>

    <br class="hidden-xs"/>



    <div class="row">
      <div class="col-sm-5 col-sm-offset-1">
        <b>Age:</b>
        <input disabled type="number" class="form-control btn-block modTextbox" id="age"  value="<%-student[0].age%>" placeholder="Age.." min=14 max=80>
      </div>

      <div class="col-sm-5">
        <b>Gender:</b>
        <select class="form-control btn-block modTextbox" id="gender">
          <option <%if(student[0].gender==='male'){%><%-'selected '%><%}%> value="male">Male</option>
          <option <%if(student[0].gender==='female'){%><%-'selected '%><%}%> value="female">Female</option>
        </select>
      </div>
    </div>
    <br class="hidden-xs"/>

    <div class="row">

        <div class="col-sm-offset-1 col-sm-5">
          <b>Batch:</b>
          <input type="text" class="form-control btn-block modTextbox" id="batchYear"  value="<%-student[0].batch%>" placeholder="Batch.."  <%if(student[0].status === 'forUpdate'){%><%-'disabled'%><%}%>>
        </div>

        <div class="col-sm-5">
          <b>Religion:</b>
          <input type="text" class="form-control btn-block modTextbox" id="hiddenReligion" value="<%-student[0].tbReligion%>">
        </div>

    </div>
    <br class="hidden-xs"/>

    <div class="row">
      <div class="col-sm-offset-1 col-sm-5">
        <b>IP Community:</b>
        <input type="text" class="form-control btn-block modTextbox" id="ipCommunity"  value="<%if(student[0].indigenousType==="" || student[0].indigenousType===null ){%><%-'None'%><%}else{%><%-student[0].indigenousType%><%}%>" placeholder="IP Community">
      </div>
    </div>


    <br class="hidden-xs"/>


    <div class="row">
      <div class="col-sm-10 col-sm-offset-1">
        <b>Current Address:</b>
        <input type="text" id="currentAddress" class="form-control btn-block modTextbox" value="<%-student[0].address%> <%-student[0].barangay%> <%-student[0].city%> <%-student[0].province%>" placeholder="address..">
      </div>
    </div><br />








    <div class="col-sm-12 modHeaderEnroll">
      <b><center>EDUCATION BACKGROUND</center></b>
    </div>


