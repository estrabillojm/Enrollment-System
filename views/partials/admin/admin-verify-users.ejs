<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.js"></script>

<script>
  $(document).ready(function(){

    dob = new Date($('#dob').val());
    var today = new Date();
    var age = Math.floor((today-dob) / (365.25 * 24 * 60 * 60 * 1000));
    $('#age').val(age);


    $('#dob').keyup(function(){
      dob = new Date($('#dob').val());
      var today = new Date();
      var age = Math.floor((today-dob) / (365.25 * 24 * 60 * 60 * 1000));
      $('#age').val(age);
    });


    <%if(full === true){%>
      $.alert({
          title: '<%-student[0].strand%> <%-student[0].section.toUpperCase()%> is already full.',
          content: 'Students: <%-studentCount.length%>/<%-settings[0].maxStudent%>',
          boxWidth: '70%',
          useBootstrap: false,
          type: 'red',
          buttons:{
            click:{
            text: 'Close',
            btnClass: 'btn-red',
            action: function(){

            }
          }}});
    <%}else{%>
      $.alert({
          title: '<%-student[0].strand%> <%-student[0].section.toUpperCase()%>',
          content: 'Students: <%-studentCount.length%>/<%-settings[0].maxStudent%>',
          boxWidth: '70%',
          useBootstrap: false,
          type: 'green',
          buttons:{
            click:{
            text: 'Close',
            btnClass: 'btn-green',
            action: function(){

            }
          }}});
    <%}%>


    $('a.buttonRequirements').click(function(){

      var checkNum = 0;
      var reqStatus = '';

      $.each($("#formRequirements .incompleteReq[type='checkbox']:not(:checked)"), function(){

        checkNum = checkNum + 1;
        if(reqStatus === ''){
          reqStatus = reqStatus + "" + $(this).val();
        }else{
          reqStatus = reqStatus + " / " + $(this).val();
        }
      });


      if (checkNum === 0){
        alert('Please choose atleast 1 above to proceed');
        return false;
      }else{
        alert('requirements submitted');
        var lrn = $('#lrn').val();
        var lastname = $('#hiddenLastname').val();
        var firstname = $('#hiddenFirstname').val();
        var middlename = $('#hiddenMiddlename').val();
        var fullname = firstname + " " + middlename.charAt(0) + " " + lastname;
        $.post('/incomplete-requirements',{
          lrn: lrn,
          fullname: fullname,
          requirements: reqStatus
        });
      }
    });

    var dt = new Date();
    var batch = $('#batchYear').val();

    $('button.btnModEnroll').click(function(){
      var lrn = $('#lrn').val();
      var fullname = $('#fullname').val();
      var lastname = $('#hiddenLastname').val();
      var firstname = $('#hiddenFirstname').val();
      var middlename = $('#hiddenMiddlename').val();
      var section = $('#section').val();
      var strand = $('#strand').val();
      var yearLevel = $('#yearLevel').val();
      var dob = $('#dob').val();
      var phone = $('#phone').val();
      var age = $('#age').val();
      var gender = $('#gender').val();
      var ipCommunity = $('#ipCommunity').val();

      var currentAddress = $('#currentAddress').val();

      var houseNumber = $('#hiddenAddress').val();
      var barangay = $('#hiddenBarangay').val();
      var city = $('#hiddenCity').val();
      var province = $('#hiddenProvince').val();

      var elemName = $('#elementaryTr td #elemName').val();
      var elemDegree = $('#elementaryTr #elemDegree').text();
      var elemYear = $('#elementaryTr td #elemYear').val();
      var hsName = $('#highschoolTr td #hsName').val();
      var hsDegree = $('#highschoolTr #hsDegree').text();
      var hsYear = $('#highschoolTr td #hsYear').val();
      var otherName = $('#otherTr td #otherName').val();
      var otherDegree = $('#otherTr td #otherDegree').val();
      var otherYear = $('#otherTr td #otherYear').val();
      var guardianFullname = $('#guardianFullname').val();

      var gLastname = $('#hiddenGLastname').val();
      var gFirstname = $('#hiddenGFirstname').val();
      var gMiddlename = $('#hiddenGMiddlename').val();

      var guardianRelation = $('#guardianRelation').val();
      var guardianPhone = $('#guardianPhone').val();
      var guardianOccupation = $('#guardianOccupation').val();
      var fatherFullname = $('#fatherFullname').val();

      var fLastname = $('#hiddenFLastname').val();
      var fFirstname = $('#hiddenFFirstname').val();
      var fMiddlename = $('#hiddenFMiddlename').val();

      var fatherPhone = $('#fatherPhone').val();
      var fatherAddress = $('#fatherAddress').val();

